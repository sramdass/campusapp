<div id="main-column">
	<% if !@branch %>
		<%= link_to "Create School", new_branch_path %>
	<% else %>
		<h1> Branch Details </h1>
		</h2><%= @branch.name %></h2><br>
		<%= @branch.address %><br>
		<%= @branch.resource_type.name %><br>
		<p>
		  <%= link_to "Edit this Branch", edit_branch_path(@branch) %>
		</p>
	<% end %>
	
	<h1> Links </h1>
	<% if @branch %>
		<%= link_to "Quick Add Faculties", facultynew_branch_path(@branch)%>
	<% end %>
</div>
